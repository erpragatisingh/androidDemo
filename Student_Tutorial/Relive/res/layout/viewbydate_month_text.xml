<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="50dip" android:layout_height="50dip" android:focusable="false" android:focusableInTouchMode="false"
	>

	<ImageView android:id="@+id/monthImage"
	    android:layout_width="61dip"
		android:layout_height="61dip"
		android:layout_gravity="center"
		android:scaleType="centerCrop"
		>
	</ImageView>

	<TextView android:id="@+id/monthText" android:layout_width="wrap_content"
		 android:layout_height="20dip" android:layout_marginLeft="5dip"
		android:background="#00000000" android:textColor="#ffffffff" android:layout_gravity="bottom|left">
	</TextView>
</FrameLayout>
